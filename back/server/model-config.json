{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ACL": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "RoleMapping": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "Account": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "GetRoles": true,
          "CreateUserWithRole": true,
          "RegisterUser": true,
          "prototype.UpdateAccount": true,
          "prototype.DeleteAccount": true,
          "GetAllAccounts": true,
          "GetAccountsFiltered": true,
          "LogIn": true,
          "LogOut": true,
          "prototype.FirstTimeConfiguration": true,
          "GenerateRestorePasswordPinAndSendByMail": true,
          "CheckVerificationCode": true,
          "SetPassword": true,
          "prototype.ChangePassword": true,
          "RestorePassword": true,
          "VerifyEmail": true
        }
      }
    }
  },
  "AccountToken": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "Mail": {
    "dataSource": "emailConnector",
    "public": false
  },
  "Product": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "CreateProduct": true,
          "GetProducts": true,
          "prototype.UpdateProduct": true,
          "prototype.DeleteProduct": true
        }
      }
    }
  },
  "Document": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "UploadFile": true
        }
      }
    }
  },
  "Folder": {
    "dataSource": "storage",
    "public": true
  },
  "ProductImage": {
    "dataSource": "mysql",
    "public": false
  }
}
