{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ACL": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "RoleMapping": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "Account": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "GetRoles": true,
          "CreateUserWithRole": true,
          "RegisterUser": true,
          "prototype.UpdateAccount": true,
          "GetAllAccounts": true,
          "LogIn": true,
          "LogOut": true,
          "prototype.FirstTimeConfiguration": true
        }
      }
    }
  },
  "AccountToken": {
    "dataSource": "mysql",
    "public": true,
    "options": {
      "validateUpsert": true,
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  }
}
